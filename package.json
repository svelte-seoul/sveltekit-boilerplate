{
  "name": "sveltekit-boilerplate",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "svelte-kit dev",
    "build": "svelte-kit build",
    "package": "svelte-kit package",
    "preview": "svelte-kit preview",
    "prepare": "svelte-kit sync",
    "test": "vitest",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint src --ext .ts,.js,.svelte",
    "format": "prettier --write ./src/**/*.{js,svelte,ts}"
  },
  "devDependencies": {
    "@dooboo/eslint-config-svelte": "^0.0.7",
    "@playwright/test": "^1.22.2",
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/kit": "next",
    "@testing-library/svelte": "^3.1.3",
    "@types/postcss-import": "^14.0.0",
    "@types/postcss-preset-env": "^6.7.3",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "autoprefixer": "^10.4.7",
    "cssnano": "^5.1.11",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "jsdom": "^19.0.0",
    "postcss": "^8.4.14",
    "postcss-custom-media": "^8.0.2",
    "postcss-import": "^14.1.0",
    "postcss-load-config": "^4.0.1",
    "postcss-nested": "^5.0.6",
    "postcss-preset-env": "^7.7.1",
    "prettier": "^2.6.2",
    "prettier-plugin-svelte": "^2.7.0",
    "svelte": "^3.48.0",
    "svelte-check": "^2.7.2",
    "svelte-preprocess": "^4.10.7",
    "sveltekit-i18n": "^2.2.2",
    "sveltekit-theme": "^1.0.0",
    "tslib": "^2.4.0",
    "typescript": "~4.7.3",
    "vitest": "^0.14.2",
    "vitest-svelte-kit": "^0.0.6"
  }
}
